{
  "name": "@glass-frontier/client",
  "version": "0.1.0",
  "private": true,
  "description": "The Glass Frontier Client.",
  "type": "module",
  "scripts": {
    "start": "vite preview",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "lint:css:json": "stylelint \"src/**/*.{css,scss}\" -f json",
    "build": "vite build"
  },
  "keywords": [
    "glass-frontier",
    "narrative-engine",
    "langgraph",
    "temporal"
  ],
  "author": "Tsonu",
  "license": "Polyform-Noncommercial-1.0.0",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@glass-frontier/chronicle-api": "workspace:*",
    "@glass-frontier/gm-api": "workspace:*",
    "@glass-frontier/dto": "workspace:*",
    "@glass-frontier/utils": "workspace:*",
    "@mui/icons-material": "^7.3.5",
    "@mui/lab": "7.0.1-beta.19",
    "@mui/material": "^7.3.5",
    "@mui/x-data-grid": "^8.18.0",
    "@mui/x-tree-view": "^8.18.0",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "amazon-cognito-identity-js": "^6.3.16",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.5",
    "remark-gfm": "^4.0.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.0",
    "vite": "~7.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx"
    ]
  }
}
