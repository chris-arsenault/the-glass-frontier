    "@glass-frontier/narrative": "workspace:*",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "amazon-cognito-identity-js": "^6.3.7",
    "react-router-dom": "^7.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "vite": "~7.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
