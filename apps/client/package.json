  },
  "keywords": [
    "glass-frontier",
    "narrative-engine",
    "langgraph",
    "temporal"
  ],
  "author": "Tsonu",
  "license": "Polyform-Noncommercial-1.0.0",
  "dependencies": {
    "@glass-frontier/dto": "workspace:*",
