{
  "priority": 2,
  "request": {
    "method": "POST",
    "urlPath": "/v1/responses",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.text.format.name == 'beat_tracker_schema')]"
      },
      {
        "matchesJsonPath": "$[?(@.input[0].content[0].text contains '#beat:update')]"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "beat-tracker-update.json"
  }
}
