  "relationshipTypes": {
    "adjacent_to": "Two locations are physically or spatially adjacent.",
    "contains": "Subject location or container holds the target within it.",
    "contained_within": "Subject is contained within the target.",
    "connected_by": "Locations are linked by an explicit route or gate.",
    "resides_in": "Subject NPC normally lives in the target location.",
    "frequents": "Subject NPC regularly visits the target location.",
    "works_at": "Subject NPC works at the target location.",
    "controls": "Subject faction or NPC exercises authority over the target.",
    "inhabited_by": "Subject location is inhabited by the target NPC or group.",
    "member_of": "Subject NPC or ship belongs to the target faction or faith.",
    "leader_of": "Subject NPC leads the target faction or faith.",
    "agent_of": "Subject NPC acts covertly on behalf of the target faction or faith.",
    "ally_of": "Subject NPC, faction, or ship is an ally of the target.",
    "enemy_of": "Subject NPC, faction, or ship is an enemy of the target.",
    "kin_of": "Subject NPC is family or blood-related to the target NPC.",
    "mentor_of": "Subject NPC is a teacher or mentor to the target NPC.",
    "owes_favor_to": "Subject NPC or faction owes a significant favor to the target.",
    "betrayed": "Subject has betrayed the target.",
    "docked_at": "Subject ship is currently docked at or stationed at the target location.",
    "patrols": "Subject ship or faction regularly patrols the target location or region.",
    "orbits": "Subject ship or habitat orbits the target celestial body.",
    "stored_at": "Subject artifact or resource is stored at the target location.",
    "discovered_at": "Subject artifact or resource was discovered at the target location.",
    "entombed_within": "Subject artifact or remains are sealed within the target.",
    "wields": "Subject NPC or faction actively uses the target artifact.",
    "seeks": "Subject NPC, faction, or faith actively seeks the target artifact, resource, or magic.",
    "source_of": "Subject location or resource is the origin or source of the target resource or magic.",
    "depleted_of": "Subject location or resource has lost or spent the target resource.",
    "studies": "Subject NPC, faction, or faith studies the target magic, resource, artifact, or phenomenon.",
    "prohibits": "Subject faith or law forbids the target magic, artifact, practice, or behavior.",
    "sanctions": "Subject faith, faction, or law formally permits or regulates the target.",
    "worships": "Subject NPC or faction venerates the target faith, artifact, or entity.",
    "opposes": "Subject faith, faction, or law opposes the target faith, law, or practice.",
    "enforces": "Subject faction, NPC, or ship enforces the target law or edict.",
    "violates": "Subject NPC, faction, or location is in violation of the target law or edict.",
    "in_effect_in": "Subject law or edict applies within the target location or jurisdiction.",
    "cause_of": "Subject law, resource, or event is a significant cause of the target conflict.",
    "fights_in": "Subject NPC, faction, or ship actively participates in the target conflict.",
    "battleground_in": "Subject conflict is actively fought in or around the target location.",
    "claims": "Subject faction or faith claims authority over the target location, resource, or group.",
    "influences": "Subject faction, faith, or magic strongly shapes the target location or group.",
    "rumors_about": "Subject rumor concerns the target entity.",
    "spawned_by": "Subject rumor or conflict originated from the target entity or event.",
    "documents": "Subject law, rumor, or artifact records or encodes information about the target.",
    "related_to": "Subject idea, law, rumor, or conflict is broadly related to the target in theme or origin.",
    "outcome_of": "Subject is a direct outcome of the target conflict or event."
  },
