  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": { "cache": false, "persistent": true, "dependsOn": ["^dev"]  },
    "lint": {},
    "test": { "dependsOn": ["^build"] },
  },
  "globalEnv": ["OPENAI_API_KEY", "VITE_TRPC_URL", "VITE_COGNITO_USER_POOL_ID", "VITE_COGNITO_CLIENT_ID", "NARRATIVE_S3_BUCKET", "NARRATIVE_S3_PREFIX", "AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY"]
}
